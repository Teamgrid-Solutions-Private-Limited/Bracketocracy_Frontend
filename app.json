{
  "expo": {
    "name": "Bracketocracy",
    "slug": "Bracketocracy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Basketball-Icon.png",
    "scheme": [
      "bracketocracy",
      "fb1991459454608166"
    ],
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.teamgrid.bracketocracy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Basketball-Icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.teamgrid.bracketocracy"
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "1b4939ea-f390-4aae-93b2-bad2cb350c50"
      }
    }
  }
}
